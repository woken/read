<html lang="es">
	<head>
		<title>Lector de documentos</title>
		<meta charset="utf-8">
		<meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
		<meta http-equiv="Pragma" content="no-cache" />
		<meta http-equiv="Expires" content="0" />
		<meta name="ct" content="<%=ct%>">
		<link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" />
		<link type="text/css" rel="stylesheet" href="public/css/movistar.css" />
	</head>
	<body>
		<header class="navbar navbar-static-top" id="top" role="banner">
			<div class="container">
				<div class="navbar-header">
					<button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#bs-navbar" aria-controls="bs-navbar" aria-expanded="false">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					</button>
					<a href="http://www.movistar.cl/" class="navbar-brand" style="display: block;">
						<!-- <img class="" style="display: initial;height: 150%;margin-top: -3px;" src="public/images/company_logo.png"> -->
					</a>
				</div>
				<nav id="bs-navbar" class="navbar-collapse collapse" aria-expanded="false" style="height: 1px;">
					<ul class="nav navbar-nav pull-right">
						
					</ul>
					<ul class="nav navbar-nav navbar-right">
						<li><a href="#">Lector de documentos</a></li>
					</ul>
				</nav>
			</div>
		</header>
		<div class="container-fluid" style="padding-left: 0px;padding-right: 0px; background-color: #eee;">
			<div class="row">
				<div class="col-lg-12">
					<div class="jumbotron">
						<div class="row">
							<div class="col-md-6">
								<h2 style="text-align:center;">Lectura</h2>
								<div id="go_text" class="alert alert-success" role="alert" style="width:100%; height:100px;border: 5px solid transparent;color: #525659;background-color: #FFFFFF;border-color: #525659;"></div>
								<div align="center" style="margin-top:10px">
									<div class="row">
										<div id="action" class="col-md-12">
										</div>
										</br>
										</br>

										<!-- ********************************************* -->

										<div class="row">
											<nav class="navbar navbar-inverse">
												<ul class="navbar-text">
													<button id="recordButton" class="btn btn-primary">Escuchar</button>
												</ul>
											</nav>
										</div>

										<!-- ********************************************* -->

										<div style="padding:0;margin-top:1em;">
											<canvas id="canvas" width="960" height="368"/>
											</canvas>
											<div id="confusion-networks">
<!-- 												<div id="canvas-placeholder">
													Word Alternatives will appear shortly after audio transcription is started
												</div> -->
												<input id="vslider" type="range" orient="vertical"/>
											</div>
											<input id="hslider" type="range" style="display:none !important;"/>
										<!-- 	<img id="left-arrow" src="#"/>
											<img id="right-arrow" src="#"/> -->
										</div>

										<div class="form-group row tabpanel-row">
											<div class="col-lg-12">
												<div class="tab-content">
													<div role="tabpanel" class="tab-pane active" id="home2">
														<textarea readonly id="resultsText" dir="auto"></textarea>
													</div>
													<div role="tabpanel" class="tab-pane" id="profile2">
														<textarea readonly id="resultsJSON"></textarea>
													</div>
												</div>
											</div><!-- .col-lg-12 -->
										</div>

										<!-- ******************************************** -->

										<input id="recordbutton" type="button" name="recordbutton" class="btn btn-primary" value="record voz">

										<br>
										<input id="record" type="button" name="record" class="btn btn-primary" value="Comando de Voz">
										<input id="record_stop" type="button" name="record" class="btn btn-primary" value="Fin de comando">
									</div>
								</div>
							</div>
							<div class="col-md-6">
								<h2 style="text-align:left;">Documento</h2>
								<input type="file" name="pdf" id="pdf">
								<br>
								<div class="form-inline">
									<input type="button" class="form-control" name="upload_pdf" id="upload_pdf" value="Subir documento" class="btn btn-default">
									<select class="form-control" id="type_document">
										<option value="">Seleccione un tipo de documento</option>
										<option value="boleta">Boleta</option>
										<option value="contrato">Contrato</option>
									</select>	
								</div>

								<img id="loading" src="#" height="50" width="50" alt="" style="display:none">
								<div id="object_pdf"></div>
								</div>
							</div>
						</div>
						</div><!-- /.col-lg-12 -->
						</div><!-- /.row -->
						</div><!-- /.container-fluid -->
						<footer class="bs-docs-footer" role="contentinfo">
							<center>
							<strong>algun footer | <a mailto="contacto@dominio.cl">contacto@dominio.cl</a></strong>
							</center>
						</footer>
						<script src="https://code.jquery.com/jquery-2.2.3.js" integrity="sha256-laXWtGydpwqJ8JA+X9x2miwmaiKhn8tVmOVEigRNtP4=" crossorigin="anonymous"></script>
						<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
						<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
						<script src="https://webaudiodemos.appspot.com/AudioRecorder/js/recorderjs/recorder.js"></script>
						<script src="javascripts/socket_wrapper.js"></script>
						<script src="javascripts/connect_ws.js"></script>
						<script src="javascripts/upload.js"></script>
						<script src="javascripts/dialog.js"></script>
						<script src="javascripts/talk.js"></script>
						<script src="javascripts/conductor.js"></script>
						<script src="javascripts/lister_user.js"></script>
						<div id="tts_audio"></div>
					</body>
				</html>